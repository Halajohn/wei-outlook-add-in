<?xml version="1.0" encoding="utf-8" ?>
<Config>
  <Zoom>150</Zoom>

  <AutoBackupEmailFromMe>true</AutoBackupEmailFromMe>
  <MyEmailAddress>wei.hu.tw@outlook.com</MyEmailAddress>
  <EmailBackupPath>c:\EmailBackup</EmailBackupPath>
  <AttachmentBackupPath>c:\Documents</AttachmentBackupPath>

  <EnableAutoBcc>true</EnableAutoBcc>
