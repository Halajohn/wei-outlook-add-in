  <Category label="Low priority" color="olCategoryColorDarkOlive"/>

  <FixedReply label="Done" text="&lt;p class=&quot;MsoNormal&quot;&gt;Dear Requester,&lt;br&gt;&lt;br&gt;Done!&lt;br&gt;&lt;br&gt;Wei.&lt;/p&gt;"/>
